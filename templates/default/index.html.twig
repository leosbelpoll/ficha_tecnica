{% extends 'base.html.twig' %}

{% block body %}
    {% if app.request.PathInfo != "/" %}
        {% set action = path('edit', {id: app.request.attributes.get('id')}) %}
    {% else %}
        {% set action = path('homepage') %}
    {% endif %}
    <div id="container">
        <!-- Nav tabs -->
        <ul class="nav nav-tabs" role="tablist">
            <li role="presentation" class="active">
                <a href="#home" aria-controls="home" role="tab" data-toggle="tab">Entrar vehículo</a>
            </li>
            <li role="presentation">
                <a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">Vehículos entrados</a>
            </li>
            <li role="presentation">
                <a href="#messages" aria-controls="messages" role="tab" data-toggle="tab">Vehículos por pagar</a>
            </li>
            <li role="presentation">
                <a href="#settings" aria-controls="settings" role="tab" data-toggle="tab">Vehículos pagados</a>
            </li>
            <li role="presentation">
                <a href="#other" aria-controls="other" role="tab" data-toggle="tab">Vehículos por finalizar</a>
            </li>
            {% if is_granted('ROLE_ADMIN') == true %}
                <li role="presentation">
                    <a href="#archivoss" aria-controls="archivoss" role="tab" data-toggle="tab">Archivos</a>
                </li>
            {% endif %}
        </ul>

        <!-- Tab panes -->
        <div class="tab-content">
            <div role="tabpanel" class="tab-pane active" id="home">
                {{ form_start(form, {'method': 'POST', 'attr': {'id': 'unique', 'class': 'contenedor', 'novalidate':true}}) }}
                {% if form_errors(form) %}
                    <div class="alert alert-danger">{{ form_errors(form) | raw }}</div>
                {% endif %}
                {% if app.request.PathInfo != "/" %}
                    {% if is_granted('ROLE_ADMIN') == true %}
                        <a href="{{ path('cambiar', {id: app.request.attributes.get('id'), estado: 'cobrar'}) }}">
                            <button type="button" class="btn btn-primary pagar">Enviar para Pagar</button>
                        </a>
                    {% endif %}
                {% endif %}
                {% for type, messages in app.session.flashbag.all() %}
                    {% for message in messages %}
                        <div class=" alert alert-{{ type }}" style="margin-top: 25px">
                            {{ message|raw }}
                        </div>
                        {#{% if type == 'error' %}#}
                        {#&#123;&#35;<a href="{{ path('frontend_contacto') }}" class="btn btn-primary pull-right"><i class="icon-chevron-right icon-white"></i> Ir a contáctenos</a>&#35;&#125;#}
                        {#{% set data = type %}#}
                        {#{% endif %}#}
                    {% endfor %}
                {% endfor %}
                <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                    <div class="panel panel-default separa">
                        <div class="panel-heading" role="tab" id="heading1">
                            <h4 class="panel-title">
                                <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse1" aria-expanded="true" aria-controls="collapse1">
                                    <h4 class="label-cabezones pull-left" style="padding-left: 15px">
                                        <strong>Características principales</strong>
                                    </h4>
                                </a>
                            </h4>
                        </div>
                        <div id="collapse1" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading1">
                            <div class="panel-body">
                                <div class="form-inline">
                                    <div class="form-group">
                                        {{ form_widget(form.clase) }}
                                    </div>
                                    <div class="form-group">
                                        {{ form_widget(form.marca) }}
                                    </div>
                                    <div class="form-group">
                                        {{ form_widget(form.tipo) }}
                                    </div>
                                    <div class="form-group">
                                        {{ form_widget(form.grupo) }}
                                    </div>
                                </div>
                                <div class="form-inline">
                                    <div class="form-group">
                                        {{ form_widget(form.modelo) }}
                                    </div>
                                    <div class="form-group">
                                        {{ form_widget(form.fechaInicio) }}
                                    </div>
                                    <div class="form-group">
                                        <label class="label-date">al</label>
                                    </div>
                                    <div class="form-group">
                                        {{ form_widget(form.fechaFin) }}
                                    </div>
                                    <div class="form-group">
                                        <a href="{{ path('homepage') }}">
                                            <button type="button" class="btn btn-success but nuevo">Nuevo</button>
                                        </a>
                                    </div>
                                    <div class="form-group">
                                        <button type="submit" class="btn btn-primary but" id="guardame">Guardar</button>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <a href="{{ path('fos_user_security_logout') }}">
