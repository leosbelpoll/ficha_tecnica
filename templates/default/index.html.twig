                                    {% if app.request.PathInfo != "/" %}
                                        {% for file in files %}
                                            {% if file.tipo == "Imágenes" %}
                                                <div class="col-md-4 alinia">
                                                    <span data-file="{{ loop.index }}" class="glyphicon glyphicon-remove-circle close" aria-hidden="true"></span>
                                                    {% set foo = file.name | split('#') %}
                                                    {% if foo[0] == 'local' %}
                                                        {% if is_granted('ROLE_ADMIN') == true %}
                                                            <a href="{{ asset( asset(foo[1])) }}">
                                                                <span class="glyphicon glyphicon-download close" aria-hidden="true"></span></a>
